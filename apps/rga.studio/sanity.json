{
  "root": true,
  "project": {
    "name": "RGA Sanity Studio"
  },
  "api": {
    "projectId": "3x4p2vjc",
    "dataset": "production"
  },
  "plugins": [
    "@sanity/base",
    "@sanity/components",
    "@sanity/default-layout",
    "@sanity/default-login",
    "@sanity/desk-tool",
    "mux-input"
  ],
  "env": {
    "development": {
      "plugins": ["@sanity/vision"]
