{
  "name": "root",
  "description": "React GraphQL website, built with Gatsby",
  "version": "2.0.0",
  "author": "React GraphQL Academy <hello@reactgraphql.academy>",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@sanity/block-content-to-html": "^1.3.8",
    "@sanity/block-content-to-react": "^2.0.6",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/styled-components": "^4.4.3",
    "@types/styled-system": "^5.1.6",
    "final-form": "^4.9.1",
    "firebase": "^6.0.2",
    "firebase-admin": "^8.5.0",
    "firebase-functions": "^3.2.0",
    "lodash.isequal": "^4.5.0",
    "markdown-to-jsx": "^6.10.2",
    "payment": "^2.3.0",
    "prismjs": "^1.17.1",
    "raven-js": "^3.26.4",
    "react": "^16.10.0",
    "react-dom": "^16.10.0",
    "react-final-form": "^6.3.0",
    "react-helmet": "^5.2.0",
    "react-images": "^0.5.19",
    "react-lazyload": "^2.3.0",
    "react-live": "^2.1.2",
    "react-photo-gallery": "^8.0.0",
    "react-runkit": "^0.9.0",
    "react-scroll": "^1.7.10",
    "react-share": "^2.3.1",
    "react-sticky-box": "^0.9.2",
    "react-styled-flexboxgrid": "^3.1.0",
    "react-super-responsive-table": "^5.0.0",
    "react-twitter-embed": "^2.0.8",
    "rehype-react": "^4.0.1",
    "styled-components": "^5.0.0",
    "styled-system": "^5.0.5",
    "typescript": "^3.7.5"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "postinstall": "echo 'TODO fix this ðŸ”¥postinstall hack https://github.com/DefinitelyTyped/DefinitelyTyped/issues/33311#issuecomment-560230205 ðŸ”¥ in root package.json' && rm -rf node_modules/@types/react-native",
    "format": "prettier --write '{src,functions,firebase}/**/*.{js,json,css,md}'",
    "prebuild": "npm run format",
    "publish": "npx lerna publish",
    "bootstrap": "npx lerna bootstrap",
    "build:packages": "lerna run compile"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-class-properties": "^7.0.0",
    "@babel/plugin-transform-regenerator": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.4.4",
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-notes": "^5.1.3",
    "@storybook/react": "^5.0.11",
    "@testing-library/jest-dom": "^4.2.0",
    "@testing-library/react": "^9.3.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-prismjs": "^1.1.1",
    "babel-preset-gatsby": "^0.2.22",
    "common-tags": "^1.8.0",
    "cypress": "^3.1.4",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^6.6.0",
    "eslint-plugin-promise": "^4.2.1",
    "firebase-tools": "^7.3.2",
    "graphql-tools-fork": "8.0.1",
    "husky": "^1.0.0-rc.13",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.5.0",
    "jest-environment-jsdom": "^23.4.0",
    "jest-environment-jsdom-global": "^1.1.0",
    "jest-styled-components": "^7.0.0",
    "jest-transform-graphql": "^2.1.0",
    "lerna": "^3.20.2",
    "lint-staged": "^9.4.2",
    "node-fetch": "^2.2.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.12.0",
    "react-storybook-decorator-background": "^1.0.5",
    "react-test-renderer": "^16.5.0",
    "start-server-and-test": "^1.7.1",
    "wait-for-expect": "^1.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.stories.js",
      "!src/**/icons/*.js"
    ],
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      "^.+\\.jsx?$": "<rootDir>/test/jest-config/preprocess.js"
    },
    "testRegex": "/.*(__tests__\\/.*)|(.*(test|spec))\\.jsx?$",
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss|ico|eot|ttf|woff|woff2)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache",
      "cypress"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "testURL": "http://localhost",
    "testEnvironment": "jest-environment-jsdom-global",
    "setupFiles": [
      "<rootDir>/test/jest-config/loadershim.js",
      "<rootDir>/test/jest-config/setup.js"
    ]
  },
  "resolutions": {
    "**/event-stream": "^4.0.1"
  },
  "workspaces": [
    "packages/*",
    "www",
    "studio"
  ]
}
