{
  "name": "react-js-academy",
  "description": "ReactJS Academy website, built with Gatsby",
  "version": "2.0",
  "author": "ReactJS Academy <hello@reactjs.academy>",
  "dependencies": {
    "apollo-cache-inmemory": "^1.2.5",
    "apollo-client": "^2.3.5",
    "apollo-link-http": "^1.5.4",
    "final-form": "^4.9.1",
    "firebase": "4.x",
    "firebase-admin": "^5.12.1",
    "firebase-functions": "^1.1.0",
    "gatsby": "^2.3.34",
    "gatsby-plugin-google-analytics": "^2.0.19",
    "gatsby-plugin-google-tagmanager": "^2.0.13",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-source-filesystem": "^2.0.33",
    "gatsby-transformer-remark": "^2.3.12",
    "payment": "^2.3.0",
    "prop-types": "^15.6.2",
    "raven-js": "^3.26.4",
    "react": "^16.8.6",
    "react-apollo": "^2.1.9",
    "react-burger-menu": "^2.5.2",
    "react-dom": "^16.8.6",
    "react-final-form": "^4.1.0",
    "react-helmet": "^5.2.0",
    "react-images": "^0.5.19",
    "react-lazyload": "^2.3.0",
    "react-live": "^1.12.0",
    "react-photo-gallery": "^7.0.2",
    "react-runkit": "^0.9.0",
    "react-scroll": "^1.7.10",
    "react-share": "^2.3.1",
    "react-styled-flexboxgrid": "^3.1.0",
    "react-super-responsive-table": "^4.3.11",
    "react-twitter-embed": "^2.0.8",
    "react-width": "^0.1.8",
    "rehype-react": "^3.0.3",
    "store": "^2.0.12",
    "styled-components": "^4.2.0",
    "styled-system": "^4.1.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "develop": "gatsby develop",
    "start": "npm run develop",
    "predeploy": "npm run build",
    "deploy": "./node_modules/.bin/firebase deploy --only hosting",
    "format": "prettier --write '{src,functions,firebase}/**/*.{js,json,css,md}'",
    "prebuild": "npm run format",
    "build": "gatsby build",
    "test": "./node_modules/.bin/jest",
    "testw": "./node_modules/.bin/jest --watch --env=jsdom  --verbose false",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "precommit": "npm run format",
    "preserve": "yarn build",
    "serve": "gatsby serve",
    "coverage": "./node_modules/.bin/jest --coverage",
    "cy:open": "./node_modules/.bin/cypress open",
    "cy:run": "./node_modules/.bin/cypress run",
    "test:e2e": "start-server-and-test serve http://localhost:9000 cy:open",
    "ci:e2e": "start-server-and-test serve http://localhost:9000 cy:run"
  },
  "devDependencies": {
    "@babel/plugin-transform-regenerator": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/runtime": "^7.4.4",
    "@storybook/react": "^3.4.8",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.2",
    "babel-loader": "^6.4.1",
    "babel-plugin-syntax-class-properties": "^6.13.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "common-tags": "^1.8.0",
    "cypress": "^3.1.4",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.12.0",
    "eslint-plugin-promise": "^3.6.0",
    "firebase-tools": "^4.2.1",
    "gatsby-image": "^2.0.41",
    "gatsby-plugin-sharp": "^2.0.36",
    "gatsby-transformer-sharp": "^2.1.19",
    "graphql-tag": "^2.9.2",
    "husky": "^1.0.0-rc.13",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.5.0",
    "jest-environment-jsdom": "^23.4.0",
    "jest-environment-jsdom-global": "^1.1.0",
    "jest-styled-components": "^6.2.0",
    "jest-transform-graphql": "^2.1.0",
    "lint-staged": "^7.2.0",
    "node-fetch": "^2.2.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.12.0",
    "react-storybook-decorator-background": "^1.0.5",
    "react-test-renderer": "^16.5.0",
    "start-server-and-test": "^1.7.1",
    "wait-for-expect": "^1.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.stories.js",
      "!src/**/icons/*.js"
    ],
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      "^.+\\.jsx?$": "<rootDir>/test/jest-config/preprocess.js"
    },
    "testRegex": "/.*(__tests__\\/.*)|(.*(test|spec))\\.jsx?$",
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss|ico)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache",
      "cypress"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "testURL": "http://localhost",
    "testEnvironment": "jest-environment-jsdom-global",
    "setupFiles": [
      "<rootDir>/test/jest-config/loadershim.js",
      "<rootDir>/test/jest-config/setup.js"
    ]
  },
  "resolutions": {
    "**/event-stream": "^4.0.1"
  }
}
