{
  "name": "gatsby-starter-default",
  "description": "Gatsby default starter",
  "version": "1.0.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "apollo-cache-inmemory": "^1.2.5",
    "apollo-client": "^2.3.5",
    "apollo-link-http": "^1.5.4",
    "babel-loader": "^6.4.1",
    "babel-polyfill": "^6.26.0",
    "common-tags": "^1.8.0",
    "email-validator": "^2.0.4",
    "final-form": "^4.9.1",
    "firebase": "4.x",
    "firebase-admin": "^5.12.1",
    "firebase-functions": "^1.1.0",
    "gatsby": "^1.9.247",
    "gatsby-link": "^1.6.40",
    "gatsby-plugin-accessibilityjs": "^1.0.3",
    "gatsby-plugin-catch-links": "^1.0.24",
    "gatsby-plugin-google-analytics": "^1.0.31",
    "gatsby-plugin-google-tagmanager": "^1.0.19",
    "gatsby-plugin-offline": "^1.0.18",
    "gatsby-plugin-react-helmet": "^2.0.10",
    "gatsby-plugin-react-next": "^1.0.11",
    "gatsby-plugin-styled-components": "^2.0.11",
    "gatsby-source-filesystem": "^1.5.39",
    "gatsby-transformer-remark": "^1.7.44",
    "graphql-tag": "^2.9.2",
    "normalize.css": "^8.0.0",
    "payment": "^2.3.0",
    "pre-commit": "^1.2.2",
    "prop-types": "^15.6.2",
    "raven-js": "^3.26.4",
    "react-apollo": "^2.1.9",
    "react-burger-menu": "^2.5.2",
    "react-final-form": "^3.6.5",
    "react-helmet": "^5.2.0",
    "react-scroll": "^1.7.10",
    "react-storybook-decorator-background": "^1.0.5",
    "react-styled-flexboxgrid": "^2.4.0",
    "react-width": "^0.1.8",
    "store": "^2.0.12",
    "styled-components": "^3.3.3"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "develop": "gatsby develop",
    "predeploy": "npm run build",
    "deploy": "firebase deploy --only hosting",
    "format": "prettier --write '{src,functions,firebase}/**/*.{js,json,css,md}'",
    "prebuild": "npm run format",
    "build": "gatsby build",
    "test": "./node_modules/.bin/jest",
    "testw": "./node_modules/.bin/jest --watch --env=jsdom  --verbose false",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "precommit": "npm run format",
    "preserve": "yarn build",
    "serve": "gatsby serve",
    "coverage": "./node_modules/.bin/jest --coverage"
  },
  "devDependencies": {
    "@storybook/react": "^3.4.8",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.2",
    "babel-plugin-syntax-class-properties": "^6.13.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.1",
    "eslint": "^4.12.0",
    "eslint-plugin-promise": "^3.6.0",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.5.0",
    "jest-environment-jsdom": "^23.4.0",
    "jest-environment-jsdom-global": "^1.1.0",
    "jest-styled-components": "^6.2.0",
    "jest-transform-graphql": "^2.1.0",
    "lint-staged": "^7.2.0",
    "node-fetch": "^2.2.0",
    "prettier": "^1.12.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-router-dom": "^4.3.1",
    "react-test-renderer": "^16.5.0",
    "wait-for-expect": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.stories.js"
    ],
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      "^.+\\.jsx?$": "<rootDir>/test/jest-config/preprocess.js"
    },
    "testRegex": "/.*(__tests__\\/.*)|(.*(test|spec))\\.jsx?$",
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "testURL": "http://localhost",
    "testEnvironment": "jest-environment-jsdom-global",
    "setupFiles": [
      "<rootDir>/test/jest-config/loadershim.js",
      "<rootDir>/test/jest-config/setup.js"
    ]
  }
}
